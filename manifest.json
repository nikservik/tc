{
  "name": "Конструктор тренингов EXPERT.FAMILY",
  "description": "Лучший констуктор для тренингов",
  "version": "0.3",
  "icons": {
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "31.0",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write"]}
  ],
  "file_handlers": {
    "any": {
      "extensions": ["efstruct"],
      "title": "Конструктор тренингов"
    }
  }
}